{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "rootDir": "../../",
    "outDir": "./dist",
    "baseUrl": "./",
    "types": ["@sapui5/types", "jquery"],
    "paths": {
      "aiwriter/*": ["./webapp/*"],
      "#cds-models/*": ["../../@cds-models/*/index.ts"]
    }
  },
  "include": ["./webapp/**/*", "types/**/*.d.ts"],
}
