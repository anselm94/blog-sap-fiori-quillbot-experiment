{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "rootDir": "./webapp",
    "outDir": "./dist",
    "baseUrl": "./",
    "types": ["@sapui5/types", "jquery"],
    "paths": {
      "aiwriter/*": ["./webapp/*"]
    }
  },
  "include": ["./webapp/**/*", "types/**/*.d.ts"]
}
